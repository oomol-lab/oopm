{
  "name": "oopm",
  "version": "0.4.3",
  "description": "OOMOL Package Manager",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "bin": {
    "oopm": "dist/bin.js"
  },
  "publishConfig": {
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup-node",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "oomol",
    "package manager"
  ],
  "author": "Kevin Cui <bh@bugs.cc>",
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "4.4.0",
    "@types/node": "22.13.9",
    "@vitest/coverage-v8": "3.0.7",
    "@vitest/ui": "3.0.7",
    "eslint": "9.21.0",
    "fastify": "5.2.1",
    "get-port": "7.1.0",
    "tsup": "8.4.0",
    "typescript": "5.8.2",
    "vitest": "3.0.7"
  },
  "packageManager": "pnpm@9.11.0",
  "dependencies": {
    "commander": "^13.1.0",
    "execa": "^9.5.0",
    "globby": "^14.0.2",
    "tar": "^7.4.3",
    "yaml": "^2.6.0"
  }
}
